<?xml version="1.0"?>
<configuration>
  <system.web>
    <customErrors mode="Off"/>
    <compilation debug="true"/>
  </system.web>
  <system.webServer>
    <defaultDocument>
      <files>
        <add value="Default.html"/>
      </files>
    </defaultDocument>
    <rewrite>
      <rules>
        <rule name="Redirect About">
          <match url="About$"/>
          <action type="Rewrite" url="default.html"/>
        </rule>
        <rule name="Redirect Services">
          <match url="Services$"/>
          <action type="Rewrite" url="default.html"/>
        </rule>
        <rule name="Redirect Contact">
          <match url="Contact$"/>
          <action type="Rewrite" url="default.html"/>
        </rule>
      </rules>
    </rewrite>
    <httpProtocol>
      <customHeaders>
        <add name="Access-Control-Allow-Origin" value="*"/>
      </customHeaders>
    </httpProtocol>
    <modules>
      <remove name="WebDAVModule"/>
    </modules>
    <handlers>
      <remove name="WebDAV"/>
    </handlers>
  </system.webServer>
</configuration>